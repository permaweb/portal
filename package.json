{
	"name": "portal",
	"version": "1.0.0",
	"description": "",
	"main": "index.tsx",
	"author": "",
	"license": "ISC",
	"scripts": {
		"clean:install": "rm -rf node_modules package-lock.json || true && npm cache clean --force && npm install",
		"format": "eslint --fix . && npx prettier --write .",
		"lint:lang": "node ./scripts/sort-language.js --check",
		"fix:lang": "node ./scripts/sort-language.js",
		"start:editor": "NODE_ENV=development VITE_APP=editor VITE_AO=legacy vite",
		"start:editor:ao-legacy": "NODE_ENV=development VITE_APP=editor VITE_AO=legacy vite",
		"start:editor:ao-mainnet": "NODE_ENV=development VITE_APP=editor VITE_AO=mainnet vite",
		"start:viewer": "NODE_ENV=development VITE_APP=viewer vite",
		"start:engine": "NODE_ENV=development VITE_APP=engine VITE_AO=legacy vite",
		"build:editor": "NODE_ENV=production VITE_APP=editor VITE_AO=legacy vite build",
		"build:viewer": "NODE_ENV=production VITE_APP=viewer VITE_AO=legacy vite build",
		"build:engine": "NODE_ENV=production VITE_APP=engine VITE_AO=legacy vite build",
		"start:editor:testnet": "NODE_ENV=development VITE_APP=editor VITE_TESTNET=true vite",
		"start:viewer:testnet": "NODE_ENV=development VITE_APP=viewer VITE_TESTNET=true vite",
		"build:editor:testnet": "NODE_ENV=production VITE_APP=editor VITE_TESTNET=true vite build",
		"build:viewer:testnet": "NODE_ENV=production VITE_APP=viewer VITE_TESTNET=true vite build",
		"deploy:editor:main": "VITE_APP=editor npm run build:editor && permaweb-deploy --arns-name portal --ttl-seconds 60 --deploy-folder ./dist/editor",
		"deploy:editor:staging": "VITE_APP=editor npm run build:editor && permaweb-deploy --arns-name portal --ttl-seconds 60 --undername staging --deploy-folder ./dist/editor",
		"deploy:viewer": "VITE_APP=viewer npm run build:viewer && permaweb-deploy --arns-name portal --ttl-seconds 60 --undername script --deploy-file ./dist/viewer/bundle.js",
		"deploy:engine": "VITE_APP=engine npm run build:engine && permaweb-deploy --arns-name portal --ttl-seconds 60 --undername engine --deploy-file ./dist/engine/bundle.js"
	},
	"dependencies": {
		"@ar.io/sdk": "^3.16.0-alpha.1",
		"@hello-pangea/dnd": "^17.0.0",
		"@lexical/link": "^0.34.0",
		"@lexical/list": "^0.34.0",
		"@lexical/plain-text": "^0.34.0",
		"@lexical/react": "^0.34.0",
		"@lexical/rich-text": "^0.34.0",
		"@lexical/selection": "^0.34.0",
		"@lexical/utils": "^0.34.0",
		"@permaweb/aoconnect": "0.0.90",
		"@permaweb/libs": "^0.0.53",
		"@stripe/react-stripe-js": "^3.5.1",
		"@stripe/stripe-js": "^6.1.0",
		"@wanderapp/connect": "^0.1.11",
		"arweave": "^1.15.5",
		"draft-js": "^0.11.7",
		"html-react-parser": "^5.2.5",
		"lexical": "^0.34.0",
		"react": "^18.2.0",
		"react-colorful": "^5.6.1",
		"react-dom": "^18.2.0",
		"react-markdown": "^8.0.7",
		"react-redux": "^7.2.9",
		"react-router-dom": "^6.4.2",
		"react-svg": "^15.1.11",
		"redux-persist": "^6.0.0",
		"redux-thunk": "^2.4.2",
		"styled-components": "^5.3.6",
		"webfontloader": "^1.6.28"
	},
	"devDependencies": {
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@types/react-router-dom": "^5.3.3",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@vitejs/plugin-react": "^4.4.1",
		"eslint": "^8.35.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"husky": "^8.0.0",
		"permaweb-deploy": "2.3.0",
		"prettier": "^2.8.3",
		"rollup-plugin-polyfill-node": "^0.13.0",
		"vite": "^6.3.5",
		"vite-plugin-node-polyfills": "^0.23.0",
		"vite-plugin-singlefile": "^2.2.0"
	}
}
